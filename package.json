{
  "name": "@migerh/js-module-walker",
  "version": "0.0.1",
  "description": "Analyze dependencies of es6 module based js projects",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./ --ignore build,node_modules,tests,feature,coverage,tmp --out-dir ./build",
    "start": "./node_modules/.bin/babel-node index.js",
    "test": "./tests.sh",
    "prepublish": "npm run build"
  },
  "bin": {
    "js-module-walker": "./build/index.js"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "index.js",
      "lib/**/*.js"
    ],
    "concurreny": 5,
    "babel": "inherit",
    "verbose": true,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "espower",
      "transform-runtime"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "exclude": [
      "tests/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "bugs": {
    "url": "https://github.com/migerh/js-module-walker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/migerh/js-module-walker.git"
  },
  "author": "Michael Gerhaeuser <michael.gerhaeuser@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "commander": "^2.9.0",
    "lodash": "^4.17.3"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.7",
    "stream-buffers": "^3.0.1",
    "uuid": "^3.0.1"
  }
}
